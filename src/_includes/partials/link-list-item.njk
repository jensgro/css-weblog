<li>
  {% if post.data.icon %}
  <svg class="svg-icon svg-icon--medium"><use xlink:href="/assets/svg/webdev1.svg#{{post.data.icon}}"></use></svg>
  {% else %}
  <svg class="svg-icon svg-icon--medium"><use xlink:href="/assets/svg/webdev1.svg#code"></use></svg>
  {% endif %}
  <a href="{{ post.url }}">{{ post.data.title }}</a>
  <span class="list-item-meta">
    {{ post.data.date | htmlDateString }}
  </span>
</li>