---
layout: layouts/base.njk
templateClass: tmpl-post
---
<div class="container">
  <main class="post-cnt">
    <div class="codepen-overview">
      {{ content | safe }}
    </div>

    {% if pens %}
    <ul class="codepen-list">
      {% for pen in pens %}
      <li><a href="{% pen.url | url %}">{% pen.name %}</a></li>
      {% endfor %}
    </ul>
    {% endif %}
  </main>
</div>



